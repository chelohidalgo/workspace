<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>accountController</managed-bean-name>
  <managed-bean-class>net.softwareevolutivo.web.controller.AccountController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>withdrawSession</managed-bean-name>
  <managed-bean-class>net.softwareevolutivo.web.controller.session.WithdrawSession</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>withdrawController</managed-bean-name>
  <managed-bean-class>net.softwareevolutivo.web.controller.WithdrawController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>withdrawSession</property-name>
   <property-class>net.softwareevolutivo.web.controller.session.WithdrawSession</property-class>
   <value>#{withdrawSession}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>moneyController</managed-bean-name>
  <managed-bean-class>net.softwareevolutivo.web.controller.MoneyController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>moneySession</property-name>
   <property-class>net.softwareevolutivo.web.controller.session.MoneySession</property-class>
   <value>#{moneySession}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>moneySession</managed-bean-name>
  <managed-bean-class>net.softwareevolutivo.web.controller.session.MoneySession</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>historyRequestController</managed-bean-name>
  <managed-bean-class>net.softwareevolutivo.web.controller.HistoryRequestController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/pages/home.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>menu</from-outcome>
   <to-view-id>/pages/menu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>pageRequest</from-outcome>
   <to-view-id>/pages/request.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/menu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>income</from-outcome>
   <to-view-id>/pages/registerIncome.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>history</from-outcome>
   <to-view-id>/pages/historyRequest.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/registerIncome.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/menu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/request.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/historyRequest.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/menu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
