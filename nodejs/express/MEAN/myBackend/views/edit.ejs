<% include header %>
<form action="/users/edit" method="POST">
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <input type="text" placeholder="Name" name="name" value="<%= user.name %>">
  <input type="text" placeholder="Last name" name="lastname" value="<%= user.lastname %>">
  <input type="text" placeholder="Id" name="id" value="<%= user.id %>">
  <input type="text" placeholder="Age" name="age" value="<%= user.age %>">
  <button type="submit">Submit</button>
</form>
<% include footer%>